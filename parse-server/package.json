    "start": "yarn build && node ./build/index.js",
    "dev": "babel-node ./server/index.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "parse-dashboard": "^2.1.0",
    "parse-server": "^4.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
